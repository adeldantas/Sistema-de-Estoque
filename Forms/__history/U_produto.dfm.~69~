  inherited Q_padrao: TFDQuery
    Active = True
    UpdateOptions.AssignedValues = [uvGeneratorName]
    SQL.Strings = (
      'SELECT A.ID_PRODUTO,'
      '       A.PRODUTO_DESCRICAO,'
      '       A.VL_CUSTO,'
      '       A.VL_VENDA,'
      '       A.ESTOQUE,'
      '       A.ESTOQUE_MIN,'
      '       A.UNIDADE,'
      '       A.CADASTRO,'
      '       A.ID_FORNECEDOR'
      ' FROM PRODUTO A, FORNECEDOR B'
      ' WHERE A.ID_FORNECEDOR=B.ID_FORNECEDOR')
    Left = 776
    Top = 72
