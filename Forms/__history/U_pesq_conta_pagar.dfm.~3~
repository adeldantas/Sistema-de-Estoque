  inherited Q_pesq_padrao: TFDQuery
    SQL.Strings = (
      'SELECT'
      '     A.ID_COMPRA,'
      '     A.ID_FORNECEDOR,'
      '     B.NOME,'
      '     B.CNPJ,'
      '     C.ID_SEQUENCIA,'
      '     C.VALOR_PARCELA,'
      '     C.DT_VENCIMENTO,'
      '     C.DT_PAGAMENTO,'
      '     C.ATRASO,'
      '     C.JUROS,'
      '     C.VL_JUROS,'
      '     C.TOTAL_PAGAR,'
      '     C.STATUS'
      '  FROM COMPRA A'
      '  INNER JOIN FORNECEDOR B ON B.ID_FORNECEDOR = A.ID_FORNECEDOR'
      '  INNER JOIN CONTAS_PAGAR C ON C.ID_COMPRA = A.ID_COMPRA'
      '  WHERE C.STATUS='#39'EM ABERTO'#39
      '  ORDER BY A.ID_COMPRA')
