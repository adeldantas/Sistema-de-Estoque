  inherited Q_padrao_item: TFDQuery
    Active = True
    IndexFieldNames = 'ID_VENDA'
    AggregatesActive = True
    MasterFields = 'ID_VENDA'
    DetailFields = 'ID_VENDA'
    Connection = DM.conexao
    SQL.Strings = (
      'SELECT'
      '        ID_SEQUENCIA,'
      '        ID_VENDA,'
      '        ID_PRODUTO,'
      '        QTDE, '
      '        VL_VENDA,'
      '        DESCONTO,'
      '        TOTAL_ITEM   '
      'FROM ITEM_VENDA'
      'WHERE ID_VENDA=:ID_VENDA'
      'ORDER BY ID_SEQUENCIA')
    Left = 792
    ParamData = <
      item
        Name = 'ID_VENDA'
        DataType = ftInteger
